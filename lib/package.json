{
  "name": "@vapurrmaid/bpm",
  "version": "0.1.0",
  "description": "Lightweight package for working with beats per minute (BPM) calculations.",
  "keywords": [
    "bpm",
    "music",
    "tempo"
  ],
  "homepage": "https://github.com/vapurrmaid/bpm/tree/master/lib",
  "bugs": {
    "email": "vapurrmaid@pm.me",
    "url": "https://github.com/vapurrmaid/bpm/tree/master/lib"
  },
  "license": "MIT",
  "author": {
    "email": "vapurrmaid@pm.me",
    "name": "Gregory (Grey) Barkans",
    "url": "https://github.com/vapurrmaid"
  },
  "files": [
    "dist"
  ],
  "main": "dist/bpm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vapurrmaid/bpm"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "release": "release-it"
  },
  "dependencies": {
    "@vapurrmaid/validate": "^0.2.0"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "release-it": "^13.0.2",
    "typescript": "^3.8.3"
  },
  "release-it": {
    "git": {
      "commitArgs": "--gpg-sign",
      "commitMessage": ":rocket: v${version}",
      "tagAnnotation": "v${version}",
      "tagArgs": "--sign"
    },
    "github": {
      "release": true,
      "releaseName": "Release ${version}",
      "tokenRef": "RELEASE_IT_GITHUB_TOKEN"
    }
  }
}
